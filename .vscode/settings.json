{
    "luau-lsp.platform.type": "roblox",
    "luau-lsp.sourcemap.enabled": true,
    "luau-lsp.types.definitionFiles": [
        "luau_definitions/_G.d.luau"
    ],
    "editor.rulers": [
        80,
    ],
    "stylua.targetReleaseVersion": "latest",
    "files.exclude": {
        "**/.vscode": true,
        ".gitignore": true,
        "mise.toml": true,
        "*.project.json": true,
        ".create": true,
        "luau_definitions": true,
        ".luaurc": true,
        "stylua.toml": true,
        "sourcemap.json": true,
        "*.rbxl": true,
        "*.lock": true,
        ".pesde": true,
        "*_packages": true,
        "pesde.toml": true,
        ".gitattributes": true,
        ".env": true,
    },
    "files.associations": {
        "**.d.luau": "text",
        ".luaurc": "json",
    },
    "files.readonlyInclude": {
        "stylua.toml": true,
        "sourcemap.json": true,
        "*.rbxl": true,
        "*.lock": true
    },
}