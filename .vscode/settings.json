{
    "luau-lsp.platform.type": "roblox",
    "luau-lsp.sourcemap.enabled": true,
    "luau-lsp.types.definitionFiles": [
        "types/_G.d.luau"
    ],
    "stylua.targetReleaseVersion": "latest",
    "files.exclude": {
        "**/.vscode": true,
        ".gitignore": true,
        "rokit.toml": true,
        "*.project.json": true,
        "scripts": true,
        "types": true,
        ".luaurc": true,
        "stylua.toml": true,
        "sourcemap.json": true,
        "*.rbxl": true,
        "*.lock": true,
        "project.toml": true
    },
    "editor.rulers": [
        80,
    ],
    "files.associations": {
        "**.d.luau": "text",
        ".luaurc": "json",
    },
    "files.readonlyInclude": {
        "stylua.toml": true,
        "sourcemap.json": true,
        "*.rbxl": true,
    },
}