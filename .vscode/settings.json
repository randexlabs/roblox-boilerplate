{
    "luau-lsp.platform.type": "roblox",
    "luau-lsp.sourcemap.enabled": true,
    "luau-lsp.types.definitionFiles": [
        "types/_G.d.luau"
    ],
    "editor.rulers": [
        80,
    ],
    "stylua.targetReleaseVersion": "latest",
    "files.exclude": {
        "**/.vscode": false,
        ".gitignore": false,
        "rokit.toml": false,
        "*.project.json": false,
        "scripts": false,
        "types": false,
        ".luaurc": false,
        "stylua.toml": false,
        "sourcemap.json": false,
        "*.rbxl": false,
        "*.lock": false,
        "project.toml": false,
        ".pesde": false,
        "*_packages": false,
        "pesde.toml": false
    },
    "files.associations": {
        "**.d.luau": "text",
        ".luaurc": "json",
    },
    "files.readonlyInclude": {
        "stylua.toml": true,
        "sourcemap.json": true,
        "*.rbxl": true,
    },
}